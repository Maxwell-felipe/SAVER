{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not firefox <= 118",
    "not safari <= 17"
  ]
}